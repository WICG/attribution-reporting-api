{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/WICG/attribution-reporting-api/aggregatable_report.schema.json",
  "title": "Aggregatable Report",
  "description": "An Attribution Reporting aggregatable report",
  "type": "object",
  "properties": {
    "aggregation_coordinator_identifier": {
      "type": "string"
    },
    "aggregation_service_payloads": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "debug_cleartext_payload": {
            "type": "string"
          },
          "key_id": {
            "type": "string"
          },
          "payload": {
            "type": "string"
          }
        },
        "required": ["key_id", "payload"]
      }
    },
    "shared_info": {
      "type": "string"
    },
    "source_debug_key": {
      "$ref": "#/$defs/unsigned_base10_integer",
      "default": null
    },
    "trigger_debug_key": {
      "$ref": "#/$defs/unsigned_base10_integer",
      "default": null
    }
  },
  "required": [
    "aggregation_coordinator_identifier",
    "aggregation_service_payloads",
    "shared_info"
  ],

  "$defs": {
    "unsigned_base10_integer": {
      "type": "string",
      "pattern": "^[0-9]+$"
    }
  }
}
